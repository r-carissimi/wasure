{
    "runtimes": [
        {
            "name": "wasmer",
            "command": "/opt/homebrew/bin/wasmer",
            "version-command": "/opt/homebrew/bin/wasmer -V",
            "desc": "Wasmer is a standalone WebAssembly runtime that can run WebAssembly binaries"
        },
        {
            "name": "wasmtime",
            "command": "wasmtime/bin/wasmtime",
            "version-command": "wasmtime/bin/wasmtime -V",
            "desc": "Wasmtime is a standalone JIT-style runtime for WebAssembly"
        },
        {
            "name": "wasmedge",
            "command": "wasmedge/bin/wasmedge",
            "version-command": "wasmedge/bin/wasmedge --version",
            "desc": "A CNCF extensible WebAssembly runtime."
        }
    ]
}